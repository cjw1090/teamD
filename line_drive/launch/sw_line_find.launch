<launch>
  <include file="$(find xycar_motor)/launch/xycar_motor.launch" />
  <!-- usb_cam -->


  <node name="usb_cam" pkg="usb_cam" type="usb_cam_node" output="screen" >
    <param name="video_device" value="/dev/videoCAM" />
    <param name="autoexposure" value="false"/>
    <param name="exposure" value="75"/>
    <param name="image_width" value="640" />
    <param name="image_height" value="480" />
    <param name="pixel_format" value="yuyv" />
    <param name="camera_frame_id" value="usb_cam" />
    <param name="io_method" value="mmap"/>
  </node>


  <!-- cam_tune -->
  <!-- <node name="auto_drive" pkg="line_drive" type="sw_h_c_b.py" output="screen"/> -->
  <node name="auto_drive" pkg="line_drive" type="newtest.py" output="screen"/>

  <!--node name="auto_drive" pkg="line_drive" type="sw_yj.py" output="screen"/-->
  <!-- <node name="auto_drive" pkg="line_drive" type="hough_drive.py" output="screen"/> -->
</launch>
